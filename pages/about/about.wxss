

.logo{
  display: block;
  width: 150rpx;
  height: 150rpx;
  margin: 50rpx auto 50rpx;
  border: 1px solid #ccc;
  border-radius: 15rpx;
}

.box{
  padding: 20rpx;
  background: #fff;
  border-bottom: 1px solid #eee;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
}

.arrow{
  width: 12rpx;
  height: 22rpx;
}

.userProtocol{
  overflow-y: scroll;
  position: fixed;
  top: 0;
  right: 0;
  height: 100%;
  padding: 20rpx 20rpx 40rpx;
  background: #fff;
  text-align: justify;
}

.userProtocol::-webkit-scrollbar {
  display: none;
}

.tit{
  margin: 20rpx 0;
  font-size: 40rpx;
}

.text{
  margin-top: 10rpx;
  font-size: 30rpx;
  color: #333;
  text-indent: 64rpx;
}

.ending{
  float: right;
  margin-left: 20rpx;
  padding: 30rpx 0;
  font-size: 30rpx;
}

.hide{
  display: none;
}